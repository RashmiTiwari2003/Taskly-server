{
    "compilerOptions": {
        "target": "ES2020",
        "module": "NodeNext",
        "rootDir": "./src",
        "moduleResolution": "NodeNext",
        "outDir": "./dist",
        "lib":["esnext"],
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": false,
        "skipLibCheck": true
    },
    "include": [
        "./src/**/*"
    ]
}